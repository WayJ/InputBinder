<?xml version="1.0" encoding="utf-8"?>
<ViewConfigs>

    <Demo
        key="demo_key"
        request-key="demo_rKey"/>

    <Student >
        <item key="name" request-key="name" required="true" required-remind="请填写姓名！"/>
        <item key="isBoy" request-key="isBoy" type="checkbox"/>
        <item key="birthday" request-key="birthday" required="true" type="date"
            required-remind="请选择出生日期！"/>
        <item key="input_btn" request-key="input_btn"  type="button"/>
        <item key="vision" request-key="vision" type="optional" parm = "{'optionalKeys':['0.6','0.8','1.0']}"/>
        <item key="vision2" request-key="student.vision2" type="optional"
            parm = "{'optionalKeys':['0.6','0.8','1.0'],'optionalValues':['6','8','10']}"/>
        <item key="multi" request-key="multi" type="multioptional"  parm = "{'optionalKeys':['语文','数学','英语']}"/>
        <item key="hasRoom" request-key="hasRoom" type="checkbox"/>
        <item key="roomNumber" request-key="roomNumber" dependent="-hasRoom"/>
        <item key="address" request-key="address"/>
    </Student>

</ViewConfigs>